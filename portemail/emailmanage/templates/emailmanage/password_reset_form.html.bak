{% extends "emailmanage/base.html" %}
{% load i18n %}

{% block content %}
<div class="LoginForm">
	<h1>请输入注册邮箱以找回密码</h1>
	<form method="post" action=".">
		{% if form.errors %}
			{% include "emailmanage/partials/errors.html" %}
		{% endif %}
		{% csrf_token %}
		{% for field in form %}
			{% include "emailmanage/partials/field.html" %}
		{% endfor %}
		<div class="button-wrapper submit">
			<input type="submit" value="下一步" />
		</div>
	</form>
</div>
{% endblock %}
